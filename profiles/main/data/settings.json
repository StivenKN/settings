{
  "editor.cursorBlinking": "expand",
  "editor.stickyScroll.enabled": false,
  "explorer.confirmDelete": false,
  "editor.guides.bracketPairs": "active",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "git.enabled": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, .gitignore",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "workbench.sideBar.location": "left",
  "window.density.editorTabHeight": "compact",
  "workbench.activityBar.location": "top",
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "One Dark Pro Night Flat",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.customLabels.enabled": true,
  "workbench.editor.customLabels.patterns": {
    "**/pages/**/*": "âšª (${dirname}) ${filename}.${extname}",
    "**/components/**/*": "ðŸŸ¢ (${dirname}) ${filename}.${extname}",
    "**/utils/**/*": "ðŸ”µ ${filename}.${extname}",
  },
  "workbench.iconTheme": "symbols",
  "symbols.hidesExplorerArrows": false,
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "editor.tabSize": 2,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.secondarySideBar.defaultVisibility": "visible",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "chat.todoListTool.enabled": true,
  "github.copilot.advanced": {},
  "github.copilot.chat.localeOverride": "es",
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "chat.mcp.autostart": "newAndOutdated",
  "chat.mcp.serverSampling": {
    "Global in Code: sequentialthinking": {
      "allowedModels": [
        "copilot/gpt-4.1"
      ]
    }
  },
  "workbench.startupEditor": "none",
  "nuxtr.piniaFiles.defaultTemplate": "setup",
  "nuxtr.vueFiles.style.alwaysScoped": true,
  "nuxtr.openItemsAfterCreation": true,
  "nuxtr.projectTemplates": [
    
  ],
  "github.copilot.chat.newWorkspace.useContext7": true,
  "github.copilot.selectedCompletionModel": "",
  "github.copilot.chat.agent.currentEditorContext.enabled": false,
}